{
  "rewrites": [
    {
      "source": "/.uncache",
      "destination": "https://profiler.lecoq.io/.uncache"
    },
    {
      "source": "/.requests",
      "destination": "https://profiler.lecoq.io/.requests"
    },
    { "source": "/.templates", "destination": "/.templates__" },
    {
      "source": "/.templates/:template",
      "destination": "/.templates_/:template"
    },
    {
      "source": "/about/query/:login",
      "destination": "https://profiler.lecoq.io/about/query/:login"
    },
    {
      "source": "/about/query/:login/:plugin",
      "destination": "https://profiler.lecoq.io/about/query/:login/:plugin"
    },
    {
      "source": "/insights/query/:login",
      "destination": "https://profiler.lecoq.io/insights/query/:login"
    },
    {
      "source": "/insights/query/:login/:plugin",
      "destination": "https://profiler.lecoq.io/insights/query/:login/:plugin"
    },
    {
      "source": "/:login([-\\w]+)",
      "destination": "https://profiler.lecoq.io/:login"
    },
    {
      "source": "/:login([-\\w]+)/:repository([-\\w]+)",
      "destination": "https://profiler.lecoq.io/:login/:repository"
    }
  ],
  "github": {
    "silent": true,
    "autoJobCancelation": true
  }
}
